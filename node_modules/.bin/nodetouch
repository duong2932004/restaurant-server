#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules/touch/bin/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules/touch/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/node_modules:/Users/nguyenduong/Documents/my_project/node_modules:/Users/nguyenduong/Documents/node_modules:/Users/nguyenduong/node_modules:/Users/node_modules:/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules/touch/bin/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules/touch/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/node_modules:/Users/nguyenduong/Documents/my_project/node_modules:/Users/nguyenduong/Documents/node_modules:/Users/nguyenduong/node_modules:/Users/node_modules:/node_modules:/Users/nguyenduong/Documents/my_project/restaurant/restaurant-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
